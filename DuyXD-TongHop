      if hrp then
         -- To·∫° ƒë·ªô khu ƒë·ªìi trong map TSB (∆∞·ªõc l∆∞·ª£ng chu·∫©n nh·∫•t hi·ªán t·∫°i)
         hrp.CFrame = CFrame.new(-485, 215, -320) 
